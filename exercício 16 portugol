programa
{
	
	funcao inicio()
	{
		real nota1, nota2 , media
		
		escreva("escreva o valor da nota 1 : ")
		leia(nota1)
		limpa()

		escreva("escreva o valor da nota 2 : ")
		leia(nota2)
		limpa()

		media = (nota1 + nota2) / 2 

		se (media >= 7 )
		{
			escreva("sua media é ",media," portanto aprovado ")
		}

		senao 
		{
			escreva("sua media é ",media,"portanto reprovado")
		}
	}
}
